{
  "expo": {
    "name": "Multi OAuth Example App",
    "slug": "multi-oauth-react-native",
    "version": "1.3.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "multioauthapp"
        ]
      },
      "bundleIdentifier": "xyz.carlosloureiro.multioauth"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "xyz.carlosloureiro.multioauth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "multioauthapp",
    "extra": {
      "eas": {
        "projectId": "04262026-d702-4aa5-bdc9-48a51a85c4c2"
      }
    }
  }
}

{
  "name": "multi-oauth-react-native",
  "version": "1.2.0",
  "keywords": [
    "react",
    "expo",
    "template",
    "typescript",
    "nativebase"
  ],
  "license": "MIT",
  "author": "Carlos Loureiro",
  "scripts": {
    "start": "expo start --clear",
    "start:client": "expo start --dev-client",
    "android": "expo start --android",
    "android:client": "expo run:android",
    "ios": "expo start --ios",
    "ios:client": "expo run:ios",
    "web": "expo start --web",
    "eject": "npx expo prebuild",
    "test": "tsc --noEmit && eslint --ignore-path .gitignore . --fix --quiet && npm run jest",
    "jest": "jest --clearCache && jest",
    "build:web": "expo export:web",
    "build:ios": "react-native run-ios --configuration=Release",
    "build:android": "react-native run-android --variant=Release"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^18.0.4",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@types/crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "expo": "~48.0.18",
    "expo-constants": "^14.2.1",
    "expo-linking": "^4.0.1",
    "expo-navigation-bar": "~2.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "native-base": "~3.4.28",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-crypto-js": "^1.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.10",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.2",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.64.12",
    "@types/react-native-crypto-js": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.41.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "prettier": "^2.8.8",
    "react-native-dotenv": "^3.4.8",
    "typescript": "^4.9.4"
  },
  "private": true
}
